#######################
#       功能设置       #
#######################

# SERVER酱,用于推送消息
USE_SCKEY=

# 切换HTTPS,为真则使用https协议
USE_HTTPS=true

# 是否使用代理(前提保证有效代理)
USE_PROXY=false
PROXY_IP=
PROXY_PORT=

# 直播间ID，用于礼物赠送
ROOM_ID=9522051

# 弹幕监控房间(为空则随机)
